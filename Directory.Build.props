<Project>
  <PropertyGroup>
		<!-- Version number used for the assemblies and the NuGet packages -->
		<!--<LibraryVersion>6.0.2110</LibraryVersion>-->

		<!--For use with Nightly packages-->
		<!--REMINDER: When packing, use the proper ISO layout, i.e yyyyMMdd-->
		<CurrentDate>$([System.DateTime]::Now.ToString(YYYYMMDD))</CurrentDate>
		
		<!--Version layout:
			6 = Major
			2110 = Minor
			210916 = Build
			-alpha = Indicates a pre-release package
		-->
		<LibraryVersion>6.0.2110</LibraryVersion>
		<!--https://docs.microsoft.com/en-us/nuget/create-packages/prerelease-packages-->
		<PackageVersion>6.2110.$(CurrentDate)-alpha</PackageVersion>

		<!--Canary only-->
		<!--<PackageVersion>6.0.2110-beta</PackageVersion>-->

		<!--Stable/Signed-->
		<!--<PackageVersion>6.0.2110</PackageVersion>-->
  </PropertyGroup>
</Project>